<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.script.mapper.UserMapper">
    <select id="selectUserByName" parameterType="String" resultType="com.script.mapper.UserMapper">
        select * from users u where u.name = #{username}
    </select>
</mapper>